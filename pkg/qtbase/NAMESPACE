
useDynLib(qtbase, .registration = TRUE)

export(qsetStyleSheet, qstyleSheet,
       qaction, qaddAction, qsetContextMenuPolicy)

export(".u_tryEval", ".u_eval", ".u_lastValue")

export(qsetDeleteOnClose)

export(testLabel)

export(qfont)

# metadata
export(qmethods, qnormalizedSignature, qproperties)
S3method("$", QObject)
S3method("$<-", QObject)

# signals
export(qconnect, qdisconnect)

# invoke
export(qinvoke)

# properties
export(qproperty, "qproperty<-")
